{
	"schemaVersion": 1,
	"id": "serverresourcepackreload",
	"version": "${version}",
	"name": "Minecraft server resourcepack reload",
	"description": "This mod reloads server resourcepack without the need to restart the entire server!",
	"authors": [
		"Mat0u5"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/Mat0u5/Minecraft-server.properties-reload"
	},
	"license": "MIT",
	"icon": "assets/serverresourcepackreload/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.mat0u5.serverresourcepackreload.Main"
		],
		"client": [
			"net.mat0u5.serverresourcepackreload.MainClient"
		],
		"fabric-datagen": [
			"net.mat0u5.serverresourcepackreload.MainDataGenerator"
		]
	},
	"mixins": [
		"serverresourcepackreload.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.14.21",
		"minecraft": "~1.21",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}